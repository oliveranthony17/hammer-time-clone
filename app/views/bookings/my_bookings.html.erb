<div class="container">

  <h2 class="text-center">My bookings</h2>

  <%# Iterate over bookings for where current_user == Booking.user %>

  <div class="row">
    <% @bookings.each do |booking| %>
      <div class="col-sm-6 col-md-4 col-lg-3"><%= render 'shared/booking', booking: booking, listing: booking.listing %></div>
    <% end %>
  </div>
</div>
